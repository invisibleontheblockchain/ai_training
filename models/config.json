{
  "model_version": "1.2.0",
  "base_architecture": "transformer",
  "hidden_size": 768,
  "num_layers": 12,
  "num_attention_heads": 12,
  "intermediate_size": 3072,
  "max_position_embeddings": 512,
  
  "autonomy_settings": {
    "autonomy_threshold": 0.85,
    "max_decision_time": 5000,
